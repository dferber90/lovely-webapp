{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          { "targets": { "browsers": ["last 2 versions", "safari >= 7"] } }
        ]
      ],
      "plugins": [
        "@babel/plugin-transform-react-jsx",
        "@babel/plugin-proposal-class-properties",
        "graphql-tag",
        ["styled-components", { "ssr": true }],
        "@babel/plugin-syntax-dynamic-import",
        "react-loadable/babel"
      ]
    }
  }
}
