{
  "name": "@wa/api",
  "excludeFromWorkspaceFlattening": true,
  "private": true,
  "version": "1.0.0",
  "description": "My Graphcool Service",
  "scripts": {
    "start": "node index.js",
    "start:local": "NODE_ENV=production node --require dotenv/config index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "now -e NODE_ENV=production -e APP_SECRET -e PRISMA_ENDPOINT -e PRISMA_SECRET --token $NOW_TOKEN --npm --public",
    "alias": "now alias --token=$NOW_TOKEN"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.2",
    "dotenv": "5.0.1",
    "global": "4.3.2",
    "graphql": "0.13.2",
    "graphql-cli": "2.15.8",
    "graphql-yoga": "1.6.1",
    "jsonwebtoken": "8.2.0",
    "prisma-binding": "1.5.16"
  },
  "devDependencies": {
    "jest": "22.4.3"
  }
}
